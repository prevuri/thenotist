%h1 Notes#new

.upload_form
  %h3 File Upload
  = form_for :notes, :html => { :id => "upload_form" }, :method => 'post', :url => { :action => "create" } do |f| 
    %p= f.label :title
    %p= f.text_field :title

    %p= f.label :description
    %p= f.text_field :description

    %p= f.label :upload_file
    %p= f.file_field :file
    
    %p= f.submit "Upload"

